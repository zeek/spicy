module Benchmark;

type InnerSize = unit {
    b: uint8;
};

public type UnitVectorSize = unit {
    length: uint64;
    inner: InnerSize[] &size=self.length;
    end_: bytes &size=3;
};

type InnerLookahead = unit {
    b: b"A";
};

public type UnitVectorLookahead = unit {
    length: uint64;
    inner: InnerLookahead[];
    end_: b"END";
};

public type Regex = unit {
    length: uint64;
    data: /A*/;
    end_: b"END";
};
