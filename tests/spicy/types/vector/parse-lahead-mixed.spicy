# @TEST-EXEC:  printf 'AAA\nBBB\nCCC\n---\nXXX\n' | spicy-driver -d %INPUT >output
# @TEST-EXEC:  btest-diff output

module HTTP;

const LINE = /[^-\n]+\n/;
const DASHES = b"---\n";

type Item = unit {
    line:   LINE;
};

public type Test = unit {
    on %init { print self; }

    lines: Item[] foreach { print "  item: %s" % $$; }
    dashes: DASHES;
    last: LINE;

    on %done { print self; }
    };
