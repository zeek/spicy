# @TEST-EXEC:  printf 'XXX\nXXX\nXXX\n---\nXXX\n' | spicy-driver -d %INPUT >output
# @TEST-EXEC:  btest-diff output

module HTTP;

const LINE = b"XXX\n";
const DASHES = b"---\n";

public type Test = unit {
    on %init { print self; }

    lines: Item[3];
    dashes: DASHES;
    last: LINE;

    on lines foreach { print "item: %s" % $$; }
    on %done { print self; }
    };

type Item = unit {
    line:   LINE;
};
