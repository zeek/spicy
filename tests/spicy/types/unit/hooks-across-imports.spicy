# @TEST-EXEC:  btest-diff output

module A;

import B;

public type tA = unit {
       a: bytes &size=5;
       B: B::tB;
       C: B::tC;

       on %init {
           print "tA";
       }

       on %done {
           print self;
       }
};

on tA::a {
    print "tA: a", self.a;
    }

on B::tB::x {
    print "tA: x-1", self.x;
    }

on B::tB::x {
    print "tA: x-2", self.x;
    }

### @TEST-START-FILE b.spicy
module B;

public type tB = unit {
       x: bytes &size=5;
       y: bytes &size=5;

       on %init {
           print "tB";
       }
};

public type tC = unit {
       z: bytes &size=2;
} &convert=self.z;

on tB::x {
    print "tB: x-1 $", self.x;
    }

on tB::x {
    print "tB: x-2 $", self.x;
    }

on tB::y {
    print "tB: y $", self.y.to_int();
    }
