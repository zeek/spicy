module Test;

import spicy;
import filter;

public type X = unit {
    b1: bytes &size=5;
    b2: bytes &size=5;
    b3: bytes &size=3;
    on %init {
        self.connect_filter(new filter::Base64Decode);
        self.connect_filter(new filter::Zlib);
    }
    on %done { print self; }
};
