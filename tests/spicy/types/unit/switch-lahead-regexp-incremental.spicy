module Mini;

type A = unit {
     x: /xa+/;
     y: /Foo/;
     on %done { print "A", self.x, self.y; }
};

type B = unit {
    x: /xa+X/;
    y: /Bar/;
    on %done { print "B", self.x, self.y; }
};

public type test = unit {
       switch {
           -> a: A;
           -> b: B;
           };
};
