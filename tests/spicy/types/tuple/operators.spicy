# Basic functionality tests for Spicy. HILTI provides the main tests for the type.

module Test;

global t = (1, 2, 3);
global t2: tuple<uint64, uint64, uint64> = (1, 2, 3);

assert t == (1, 2, 3);
assert t == t2;
assert (1.0, 2.0, 3.0) == (1, 2, 3);
assert t[1] == 2;
