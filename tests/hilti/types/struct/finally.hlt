# @TEST-EXEC: hiltic -gj %INPUT | sort >output
# @TEST-EXEC: btest-diff output
#
# @TEST-DOC: Test that struct finalizers execute as expected.

module Foo {

import hilti;

type X = struct(string name) {
    string s;

    hook void ~finally();
};

hook void X::~finally() {
    hilti::print("%s: finally 1!" % name);
}

hook void X::~finally() {
    hilti::print("%s: finally 2!" % name);
}

function void foo() {
    local X x3("x3");
}

global X x1("x1");
global strong_ref<X> x2 = new X("x2");
foo();
}
