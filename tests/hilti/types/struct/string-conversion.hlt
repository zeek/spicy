hook optional<string> Foo::_hook_to_string() {
    return self.x + "|" + self.y;
}

global Foo f = [$x="xxx", $y="yyy"];
global Bar b = [$x="xxx", $y="yyy"];

hilti::print(f);
hilti::print(b);

}
