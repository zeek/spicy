# @TEST-EXEC: hiltic -c %INPUT | grep -q 'my_regex(.*::Test::re)'
#
# @TEST-DOC: Test that direct references to regexp constants aren't folded into local temporaries. Regression test for #1396.

module Test {

import hilti;

function void my_regex(regexp r) {
    hilti::print(r);
}

const re = /abc/;

my_regex(re);

}
